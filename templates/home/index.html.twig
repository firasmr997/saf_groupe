<section class="section colored" id="contact-us">
    <div class="container">
        <!-- ***** Section Title Start ***** -->
        <div class="row">
            <div class="col-lg-12">
                <div class="center-heading">
                    <h2 class="section-title" style="color: black">CONTACT</h2>
                </div>
            </div>
            <div class="offset-lg-3 col-lg-6">
                <div class="center-text">
                    <p>
                        Nous ne pouvons pas résoudre votre problème si vous ne nous en parlez pas</p>
                </div>
            </div>
        </div>
        <!-- ***** Section Title End ***** -->

        <div class="row">
            <!-- ***** Contact Text Start ***** -->
            <div class="col-lg-4 col-md-6 col-sm-12">
                <h5 class="margin-bottom-30">Rester en contact
                </h5>
                <div class="contact-text">
                    <p>Adresse : Rue des trembleaux 78500, Sartouville, France</p>
                    <p>Téléphone : <span style="color: darkblue">0659339494</span> /<span style="color: darkblue"> 0645664035</span></p>
                    <p>Email : <span style="color: darkblue">contact@safgroupe.fr</span></p>
                </div>
            </div>
            <!-- ***** Contact Text End ***** -->

            <!-- ***** Contact Form Start ***** -->
            <div class="col-lg-8 col-md-6 col-sm-12">
                <div class="contact-form">
{#                    <form id="contact" action="">#}
                        {{ form_start(form) }}

                        <div class="row">
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <fieldset>
                                    {{ form_row(form.fullName, {'attr': {'class': 'form-control', 'placeholder': 'Nom'}}) }}
                                </fieldset>
                            </div>
                            <div class="col-lg-6 col-md-12 col-sm-12">
                                <fieldset>
                                    {{ form_row(form.email, {'attr': {'class': 'form-control', 'placeholder': 'Email'}}) }}

                                    {#                                    <input name="email" type="email" class="form-control" id="email"#}
{#                                           placeholder="E-Mail " required="">#}
                                </fieldset>
                            </div>
                            <div class="col-lg-12">
                                <fieldset>
                                    {{ form_row(form.subject, {'attr': {'class': 'form-control', 'placeholder': 'Objet'}}) }}
{#                                    <input name="objet" type="text" class="form-control" id="objet"#}
{#                                           placeholder="Objet " required="">#}
                                </fieldset>
                            </div>
                            <div class="col-lg-12">
                                <fieldset>
                                    {{ form_row(form.message, {'attr': {'class': 'form-control', 'placeholder': 'Message'}}) }}

                                    {#                                    <textarea name="message" rows="6" class="form-control" id="message"#}
{#                                              placeholder="Message" required=""></textarea>#}
                                </fieldset>
                            </div>
                            <div class="col-lg-12">
                                <fieldset>
                                    {{ form_row(form.envoyer, {'attr': {'class': 'main-button'}}) }}
{#                                    <button type="submit" id="form-submit" class="main-button">Envoyer</button>#}
                                </fieldset>
                            </div>
                        </div>
                        {{ form_end(form) }}
{#                    </form>#}
                </div>
            </div>
            <!-- ***** Contact Form End ***** -->
        </div>
    </div>
</section>
